{
    "name": "wasiliana/laravel-sdk",
    "description": "This package is built to make it easier to interact with Wasiliana Api for Laravel developers.",
    "license": "MIT",
    "authors": [
        {
            "name": "Wasiliana Engineering Team",
            "email": "engineering@wasiliana.com",
            "homepage": "https://github.com/Wasiliana"
        }
    ],
    "homepage": "https://github.com/wasiliana/laravel-sdk",
    "keywords": ["Laravel", "LaravelSdk"],
    "require": {
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "guzzlehttp/guzzle": "^7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Wasiliana\\LaravelSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wasiliana\\LaravelSdk\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wasiliana\\LaravelSdk\\LaravelSdkServiceProvider"
            ],
            "aliases": {
                "Sms": "Wasiliana\\LaravelSdk\\Facades\\Sms"
            }
        }
    }
}
